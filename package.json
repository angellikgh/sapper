{
  "name": "sapper",
  "version": "0.26.0-alpha.3",
  "description": "Military-grade apps, engineered by Svelte",
  "bin": {
    "sapper": "./sapper"
  },
  "files": [
    "*.js",
    "webpack",
    "config",
    "sapper",
    "dist/*.js",
    "runtime/*.mjs",
